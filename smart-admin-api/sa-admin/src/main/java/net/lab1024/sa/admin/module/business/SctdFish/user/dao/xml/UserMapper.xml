<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yy.dao.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yy.entity.User">
        <id column="id" property="id" />
        <result column="userName" property="userName" />
        <result column="passwd" property="passwd" />
        <result column="avatarId" property="avatarId" />
        <result column="nickName" property="nickName" />
        <result column="sex" property="sex" />
        <result column="email" property="email" />
        <result column="idCard" property="idCard" />
        <result column="phone" property="phone" />
        <result column="enable" property="enable" />
        <result column="createTime" property="createTime" />
        <result column="updateTime" property="updateTime" />
        <result column="roleId" property="roleId" />
        <result column="wxId" property="wxId" />
        <result column="isDelete" property="isDelete" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, userName, passwd, avatarId, nickName, sex, email, idCard, phone, enable, createTime, updateTime, roleId, wxId, isDelete
    </sql>

</mapper>
